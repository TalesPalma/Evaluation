<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".esportFragment"
    android:background="@color/background"
    >


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/IpSupino"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColorHint="@color/inputSupino"
        app:boxBackgroundMode="outline"
        app:boxStrokeColor="#FFF6F6"
        app:hintTextColor="@color/inputSupino"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.177">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/inputEDT1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Supino"
            android:inputType="numberDecimal"
            android:textColor="#FFF9F9"
            android:textColorHint="#FFF5F5"
            android:textSize="20sp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/IpAchamento"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColorHint="@color/inputAgachamento"
        app:boxStrokeColor="#FFFCFC"
        app:hintTextColor="@color/inputAgachamento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.346">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/inputEDT2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Agachamento"
            android:inputType="numberDecimal"
            android:textColor="#FBF8F8"
            android:textSize="20sp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/IpTerra"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColorHint="@color/inputTerra"
        app:boxStrokeColor="@color/white"
        app:hintTextColor="@color/inputTerra"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/inputEDT3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Levantamento terra"
            android:inputType="numberDecimal"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />
    </com.google.android.material.textfield.TextInputLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="361dp"
        android:layout_height="87dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttoSalvar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Salvar"
        android:textSize="20dp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.653"
        android:backgroundTint="@color/bluetheme"
        android:textAlignment="center"
        />

    <WebView
        android:id="@+id/grafico"
        android:layout_width="415dp"
        android:layout_height="184dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>